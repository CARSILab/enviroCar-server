{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "http://schema.envirocar.org/tracks.json#",
    "type": "object",
    "additionalProperties": false,
    "required": [
        "tracks"
    ],
    "properties": {
        "tracks": {
            "type": "array",
            "minItems": 0,
            "items": {
                "type": "object",
                "required": [
                    "id"
                ],
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "modified": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "name": {
                        "type": "string"
                    }
                }
            }
        }
    }
}
